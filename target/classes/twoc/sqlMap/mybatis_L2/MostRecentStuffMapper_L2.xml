<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.go.nims.ccm.qms.mybatis_L2.MostRecentStuffMapper_L2">

<!--     
 -->
	<resultMap type="kr.go.nims.ccm.qms.domain.MostRecentStuffVO" id="up2dateObj">
        <result property="lateststuff" column="lateststuff" />
    </resultMap>

  <select id="seekLatestStuff_L2Daily" parameterType="string" resultMap="up2dateObj">
  	SELECT 
  		TO_CHAR(max(END_DATE) ,'YYYY-MM-DD HH24:MI:SS') as lateststuff
		FROM CLD_MOD35;
  </select>
  
  
</mapper>